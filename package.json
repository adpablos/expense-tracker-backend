{
  "name": "expense-tracker-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "init-db": "ts-node scripts/init-db.ts",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "test:unit": "jest --config jest.unit.config.ts --reporters=default --reporters=jest-summarizing-reporter",
    "test:unit:specific": "jest --config jest.unit.config.ts --reporters=default --reporters=jest-summarizing-reporter --",
    "test:coverage": "jest --config jest.unit.config.ts --coverage",
    "test:integration": "docker-compose -f docker-compose.test.yml up -d --force-recreate && sleep 2 && jest --config jest.integration.config.ts --runInBand --detectOpenHandles --reporters=default --reporters=jest-summary-reporter",
    "test:integration:specific": "npm run test:integration -- --testPathPattern",
    "test:integration:cleanup": "docker-compose -f docker-compose.test.yml down",
    "test": "npm run test:unit && npm run test:integration && npm run test:integration:cleanup",
    "export-openapi": "ts-node scripts/export-openapi.ts",
    "prepare-prod-db": "ts-node scripts/prepare_production_db.ts",
    "lint": "eslint \"src/**/*.{js,ts}\" \"tests/**/*.{js,ts}\"",
    "lint:src": "eslint \"src/**/*.{js,ts}\"",
    "lint:tests": "eslint \"tests/**/*.{js,ts}\"",
    "lint:fix": "eslint \"src/**/*.{js,ts}\" \"tests/**/*.{js,ts}\" --fix",
    "lint:fix:src": "eslint \"src/**/*.{js,ts}\" --fix",
    "lint:fix:tests": "eslint \"tests/**/*.{js,ts}\" --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\" \"tests/**/*.{js,jsx,ts,tsx}\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ffprobe-installer/ffprobe": "^2.1.2",
    "axios": "^1.7.4",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-jwt": "^8.4.1",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "inversify": "^6.0.2",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.52.7",
    "pg": "^8.12.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "uuid": "^10.0.0",
    "winston": "^3.13.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.3",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/inversify": "^2.0.33",
    "@types/jest": "^29.5.13",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.7.4",
    "@types/pg": "^8.11.8",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^29.7.0",
    "jest-mock": "^29.7.0",
    "jest-summarizing-reporter": "^1.1.4",
    "jest-summary-reporter": "^0.0.2",
    "nodemon": "^3.1.4",
    "pg-mem": "^3.0.2",
    "prettier": "^3.2.5",
    "reflect-metadata": "^0.2.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  }
}
